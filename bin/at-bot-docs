#!/bin/bash
# AT-bot Documentation Compiler Wrapper
# Provides convenient access to documentation compilation

set -e

# Detect script location
SCRIPT_DIR="$(cd "$(dirname "${BASH_SOURCE[0]}")" && pwd)"
PROJECT_ROOT="$(dirname "$SCRIPT_DIR")"
LIB_DIR="$PROJECT_ROOT/lib"

# Source the documentation compiler
# shellcheck source=../lib/doc.sh
source "$LIB_DIR/doc.sh"
